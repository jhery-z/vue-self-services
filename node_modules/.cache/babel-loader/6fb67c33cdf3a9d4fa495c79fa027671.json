 Lakukan permintaan (request) ke API menggunakan Axios\n//     axios.get('https://recruitment.dev.rollingglory.com/api/v2/gifts?page[number]=1&page[size]=6')\n//       .then(response => {\n//         // Simpan data gift yang diperoleh dari API ke dalam variabel gifts\n//         this.gifts = response.data.data;\n//       })\n//       .catch(error => {\n//         console.error('Error:', error);\n//       });\n//   }\n// }\n\n\ndata: {\n    gifts: [],\n    currentPage: 1,\n    itemsPerPage: 6,\n    totalPages: 1\n  },\n  mounted() {\n    this.fetchData();\n  },\n  computed: {\n    displayedProducts() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.products.slice(start, end);\n    }\n  },\n  methods: {\n    fetchData() {\n      axios.get('https://recruitment.dev.rollingglory.com/api/v2/gifts?page[number]=1&page[size]=20')\n        .then(response => {\n          this.products = response.data.data;\n          this.totalPages = response.data.meta.totalPages;\n        })\n        .catch(error => {\n          console.error('Error:', error);\n        });\n    },\n    changePage(pageNumber) {\n      if (pageNumber >= 1 && pageNumber <= this.totalPages) {\n        this.currentPage = pageNumber;\n      }\n    }\n  }\n}\n\n</script>"]},"metadata":{},"sourceType":"module"}