{"ast":null,"code":"import NavbarMenu from \"@/components/NavbarMenu.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"KeranJang\",\n  components: {\n    NavbarMenu\n  },\n\n  data() {\n    return {\n      keranjangs: [],\n      pesan: {},\n      dismissSecs: 10,\n      dismissCountDown: 0,\n      showDismissibleAlert: true\n    };\n  },\n\n  methods: {\n    //Alert \n    goToHome() {\n      this.$router.push('/FoodAll'); //if the route accepts params, you can also use\n\n      this.$router.push({\n        name: 'FoodAll'\n      });\n    },\n\n    showAlert() {\n      this.dismissCountDown = this.dismissSecs;\n    },\n\n    setKeranjangs(data) {\n      this.keranjangs = data;\n    },\n\n    hapusKeranjang(id) {\n      axios.delete(\"http://localhost:3000/keranjangs/\" + id).then(response => {\n        console.log(\"Hapus\", response);\n        this.$toast.error('Pesanan Berhasil di Hapus', {\n          type: 'error',\n          position: 'top-right',\n          duration: 3000,\n          dismissable: true\n        }); // undate data kerajang\n\n        axios.get(\"http://localhost:3000/keranjangs\").then(response => this.setKeranjangs(response.data)).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    checkout() {\n      console.log(\"Pesan\", this.pesan);\n\n      if (this.pesan.nama && this.pesan.noMeja) {\n        // console.log(\"cek\");\n        this.pesan.keranjangs = this.keranjangs;\n        axios.post(\"http://localhost:3000/pesanans/\", this.pesan).then(() => {\n          // menghapus notif setelah berhasil pesan\n          this.keranjangs.map(function (item) {\n            return axios.delete(\"http://localhost:3000/keranjangs/\" + item.id).catch(error => console.log(error));\n          });\n          this.$router.push({\n            path: \"/pesanan-sukses\"\n          });\n          this.$toast.success('Pesanan sudah di pesan', {\n            type: 'success',\n            position: 'top-right',\n            duration: 3000,\n            dismissable: true\n          });\n        }).catch(error => console.log(error));\n      } else {\n        this.$toast.error('Nama dan No meja harus di isi', {\n          type: 'error',\n          position: 'top-right',\n          duration: 3000,\n          dismissable: true\n        });\n      }\n    }\n\n  },\n\n  mounted() {\n    axios.get(\"http://localhost:3000/keranjangs\").then(response => this.setKeranjangs(response.data)).catch(error => console.log(error));\n  },\n\n  computed: {\n    totalHarga() {\n      return this.keranjangs.reduce(function (items, data) {\n        return items + data.products.harga * data.jumlah_pesanan;\n      }, 0);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwGA;AACA;AAEA;EACAA,iBADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;MACAC,cADA;MAEAC,SAFA;MAGAC,eAHA;MAIAC,mBAJA;MAKAC;IALA;EAOA,CAbA;;EAcAC;IACA;IACAC;MACA,8BADA,CAEA;;MACA;QAAAV;MAAA;IACA,CANA;;IAUAW;MACA;IACA,CAZA;;IAaAC;MACA;IACA,CAfA;;IAgBAC;MACAC,MACAC,MADA,CACA,wCADA,EAEAC,IAFA,CAEAC;QACAC;QACA;UACAC,aADA;UAEAC,qBAFA;UAGAC,cAHA;UAIAC;QAJA,GAFA,CASA;;QACAR,MACAS,GADA,CACA,kCADA,EAEAP,IAFA,CAEAC,6CAFA,EAGAO,KAHA,CAGAC,2BAHA;MAIA,CAhBA,EAiBAD,KAjBA,CAiBAC,2BAjBA;IAkBA,CAnCA;;IAoCAC;MACAR;;MACA;QACA;QACA;QACAJ,MACAa,IADA,CACA,iCADA,EACA,UADA,EAEAX,IAFA,CAEA;UAEA;UACA;YACA,aACAD,MADA,CACA,6CADA,EAEAS,KAFA,CAEAC,2BAFA;UAGA,CAJA;UAMA;YAAAG;UAAA;UACA;YACAT,eADA;YAEAC,qBAFA;YAGAC,cAHA;YAIAC;UAJA;QAMA,CAlBA,EAmBAE,KAnBA,CAmBAC,2BAnBA;MAoBA,CAvBA,MAuBA;QACA;UACAN,aADA;UAEAC,qBAFA;UAGAC,cAHA;UAIAC;QAJA;MAMA;IACA;;EArEA,CAdA;;EAqFAO;IACAf,MACAS,GADA,CACA,kCADA,EAEAP,IAFA,CAEAC,6CAFA,EAGAO,KAHA,CAGAC,2BAHA;EAIA,CA1FA;;EA2FAK;IACAC;MACA;QACA;MACA,CAFA,EAEA,CAFA;IAGA;;EALA;AA3FA","names":["name","components","NavbarMenu","data","keranjangs","pesan","dismissSecs","dismissCountDown","showDismissibleAlert","methods","goToHome","showAlert","setKeranjangs","hapusKeranjang","axios","delete","then","response","console","type","position","duration","dismissable","get","catch","error","checkout","post","path","mounted","computed","totalHarga"],"sourceRoot":"src/views","sources":["KeranJang.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <NavbarMenu :updateKeranjang=\"keranjangs\"/>\r\n\r\n        <div class=\"container\">\r\n        <div class=\"row\">\r\n            <div class=\"col-md-8 offset-md-2 text-center\">\r\n            <div>\r\n                <b-alert v-model=\"showDismissibleAlert\" variant=\"info\" dismissible>\r\n                <p>Apakah anda ingin pesan yang lain ?</p>\r\n                    <button @click=\"goToHome()\" class=\"btn btn-danger\">\r\n                    Pesan lagi\r\n                    </button>\r\n                </b-alert>\r\n            </div>\r\n            </div>\r\n        </div>\r\n        </div>\r\n\r\n        <div class=\"container\">\r\n            <div class=\"row mt-3\">\r\n                    <div class=\"col\">\r\n                        <nav aria-label=\"breadcrumb\" class=\"nav-breadcrumb\"> \r\n                            <ol class=\"breadcrumb\">\r\n                                <li class=\"breadcrumb-item\">\r\n                                    <router-link to=\"/\" class=\"text-dark\">Home</router-link>\r\n                                </li>\r\n                                <li class=\"breadcrumb-item\">\r\n                                    <router-link to=\"/FoodAll\" class=\"text-dark\">Product All</router-link>\r\n                                </li>\r\n                                <li class=\"breadcrumb-item active\" aria-current=\"page\">\r\n                                    Keranjang\r\n                                </li>\r\n                            </ol>\r\n                        </nav>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"row\">\r\n                    <div class=\"col\">\r\n                        <h3>Pesanan Saya</h3>\r\n                        <div class=\"table-responsive mt-3\">\r\n                        <table class=\"table\">\r\n                            <thead>\r\n                                <tr>\r\n                                <th scope=\"col\">#</th>\r\n                                <th scope=\"col\">Foto</th>\r\n                                <th scope=\"col\">Makanan</th>\r\n                                <th scope=\"col\">Keterangan</th>\r\n                                <th scope=\"col\">Jumlah</th>\r\n                                <th scope=\"col\">Harga</th>\r\n                                <th scope=\"col\">Sub Total</th>\r\n                                <th scope=\"col\">Hapus</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                <tr v-for=\"(keranjang, index) in keranjangs\" :key=\"keranjang.id\">\r\n                                <th>{{index+1}}</th>\r\n                                <td>\r\n                                    <img :src=\" '../assets/img/' + keranjang.products.gambar \" class=\"img-fluid shadow\" width=\"200\"/>\r\n                                </td>\r\n                                <td>{{ keranjang.products.nama }}</td>\r\n                                <td>{{ keranjang.keterangan ? keranjang.keterangan : \"-\" }}</td>\r\n                                <td class=\"text-center\">{{ keranjang.jumlah_pesanan }}</td>\r\n                                <td>{{ keranjang.products.harga }}</td>\r\n                                <td>Rp. {{ keranjang.products.harga*keranjang.jumlah_pesanan }}</td>\r\n                                <td class=\"text-danger\"><button class=\"btn btn-danger\" @click=\"hapusKeranjang(keranjang.id)\"><b-icon-trash></b-icon-trash> Delete</button></td>\r\n                                </tr>\r\n\r\n                                <tr>\r\n                                    <td colspan=\"6\" align=\"right\">\r\n                                        <strong>Total Bayar:</strong>\r\n                                    </td>\r\n                                    <td>\r\n                                        <strong>Rp. {{ totalHarga }}</strong>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                            </table>\r\n                            </div>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"row justify-content-end\">\r\n                    <div class=\"col-md-4\">\r\n                        <form class=\"\" v-on:submit.prevent>\r\n                            <div class=\"form-group\">\r\n                                <label for=\"nama\">Nama Pemesan</label>\r\n                                <input type=\"text\" class=\"form-control\" v-model=\"pesan.nama\">\r\n                            </div>\r\n                            <div class=\"form-group\">\r\n                                <label for=\"noMeja\">Nomor Meja</label>\r\n                                <input type=\"text\" class=\"form-control\" v-model=\"pesan.noMeja\">\r\n                            </div>\r\n                            <button type=\"submit\" class=\"btn btn-success float-right\" @click=\"checkout\"><b-icon-cart></b-icon-cart> Pesan</button>\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarMenu from \"@/components/NavbarMenu.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n      name: \"KeranJang\",\r\n      components: {  \r\n        NavbarMenu,\r\n    },\r\n    data() {\r\n        return {\r\n            keranjangs: [],\r\n            pesan: {},\r\n            dismissSecs: 10,\r\n            dismissCountDown: 0,\r\n            showDismissibleAlert: true,\r\n        };\r\n    },\r\n    methods: {\r\n        //Alert \r\n        goToHome(){\r\n        this.$router.push('/FoodAll'); \r\n        //if the route accepts params, you can also use\r\n        this.$router.push({name:'FoodAll'}); \r\n      },\r\n  \r\n\r\n\r\n        showAlert() {\r\n            this.dismissCountDown = this.dismissSecs\r\n        },\r\n        setKeranjangs(data) {\r\n            this.keranjangs = data;\r\n        },\r\n    hapusKeranjang(id) {\r\n        axios\r\n        .delete(\"http://localhost:3000/keranjangs/\"+id)\r\n        .then((response) => {\r\n            console.log(\"Hapus\", response);\r\n            this.$toast.error('Pesanan Berhasil di Hapus', {\r\n                    type: 'error', \r\n                    position: 'top-right', \r\n                    duration: 3000, \r\n                    dismissable: true, \r\n                });\r\n\r\n                // undate data kerajang\r\n                axios\r\n                .get(\"http://localhost:3000/keranjangs\")\r\n                .then((response) => this.setKeranjangs(response.data))\r\n                .catch((error) => console.log(error));\r\n            })\r\n            .catch((error) => console.log(error));\r\n        },\r\n        checkout() {\r\n            console.log(\"Pesan\", this.pesan);\r\n            if(this.pesan.nama && this.pesan.noMeja) {\r\n                // console.log(\"cek\");\r\n                this.pesan.keranjangs = this.keranjangs;\r\n                axios\r\n                .post(\"http://localhost:3000/pesanans/\", this.pesan)\r\n                .then(() => {\r\n\r\n                    // menghapus notif setelah berhasil pesan\r\n                    this.keranjangs.map(function(item) {\r\n                        return axios\r\n                        .delete(\"http://localhost:3000/keranjangs/\" + item.id)\r\n                        .catch((error) => console.log(error));\r\n                    });\r\n\r\n                    this.$router.push({path: \"/pesanan-sukses\"})\r\n                    this.$toast.success('Pesanan sudah di pesan', {\r\n                        type: 'success', \r\n                        position: 'top-right', \r\n                        duration: 3000, \r\n                        dismissable: true,\r\n                    });\r\n                    })\r\n                .catch((error) => console.log(error));\r\n            }else {\r\n                this.$toast.error('Nama dan No meja harus di isi', {\r\n                    type: 'error', \r\n                    position: 'top-right', \r\n                    duration: 3000, \r\n                    dismissable: true, \r\n                });\r\n            }\r\n        }\r\n    },\r\n    mounted() {\r\n        axios\r\n        .get(\"http://localhost:3000/keranjangs\")\r\n        .then((response) => this.setKeranjangs(response.data))\r\n        .catch((error) => console.log(error));\r\n    },\r\n    computed: {\r\n        totalHarga() {\r\n            return this.keranjangs.reduce(function(items, data){\r\n                return items+(data.products.harga*data.jumlah_pesanan)\r\n            }, 0)\r\n        }\r\n    },\r\n    \r\n};\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}