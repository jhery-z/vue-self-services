{"ast":null,"code":"import NavbarMenu from \"@/components/NavbarMenu.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"FoodDetail\",\n  components: {\n    NavbarMenu\n  },\n\n  data() {\n    return {\n      product: {},\n      pesan: {}\n    };\n  },\n\n  methods: {\n    setProduct(data) {\n      this.product = data;\n    },\n\n    pesanan() {\n      console.log(this.pesan);\n\n      if (this.pesan.jumlah_pesanan) {\n        this.pesan.products = this.product;\n        axios.post(\"http://localhost:3000/keranjangs/\", this.pesan).then(() => {\n          this.$route.push({\n            path: \"/keranjang\"\n          });\n          this.$toast.success('Pesanan Berhasil di pesan', {\n            type: 'success',\n            position: 'top-right',\n            duration: 3000,\n            dismissable: true\n          });\n        }).catch(error => console.log(error));\n      } else {\n        this.$toast.error('Pesanan Gagal pesanan harus di isi', {\n          type: 'error',\n          position: 'top-right',\n          duration: 3000,\n          dismissable: true\n        });\n      }\n    }\n\n  },\n\n  mounted() {\n    axios.get(\"http://localhost:3000/products/\" + this.$route.params.id).then(response => this.setProduct(response.data)).catch(error => console.log(error));\n  }\n\n};","map":{"version":3,"mappings":"AAyDA;AACA;AAEA;EACAA,kBADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;MACAC,WADA;MAEAC;IAFA;EAIA,CAVA;;EAWAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACAC;;MACA;QACA;QACAC,MACAC,IADA,CACA,mCADA,EACA,UADA,EAEAC,IAFA,CAEA;UACA;YAAAC;UAAA;UACA;YACAC,eADA;YAEAC,qBAFA;YAGAC,cAHA;YAIAC;UAJA;QAMA,CAVA,EAWAC,KAXA,CAWAC,2BAXA;MAYA,CAdA,MAcA;QACA;UACAL,aADA;UAEAC,qBAFA;UAGAC,cAHA;UAIAC;QAJA;MAMA;IACA;;EA5BA,CAXA;;EAyCAG;IACAV,MACAW,GADA,CACA,yDADA,EAEAT,IAFA,CAEAU,0CAFA,EAGAJ,KAHA,CAGAC,2BAHA;EAIA;;AA9CA","names":["name","components","NavbarMenu","data","product","pesan","methods","setProduct","pesanan","console","axios","post","then","path","type","position","duration","dismissable","catch","error","mounted","get","response"],"sourceRoot":"src/views","sources":["FoodDetail.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <NavbarMenu />\r\n            <div class=\"container\">\r\n                <h1 class=\"text-center mt-5\">Products Detail </h1>\r\n                <div class=\"row mt-3\">\r\n                    <div class=\"col\">\r\n                        <nav aria-label=\"breadcrumb\" class=\"nav-breadcrumb\"> \r\n                            <ol class=\"breadcrumb\">\r\n                                <li class=\"breadcrumb-item\">\r\n                                    <router-link to=\"/\" class=\"text-dark\">Home</router-link>\r\n                                </li>\r\n                                <li class=\"breadcrumb-item\">\r\n                                    <router-link to=\"/FoodAll\" class=\"text-dark\">Product All</router-link>\r\n                                </li>\r\n                                <li class=\"breadcrumb-item active\" aria-current=\"page\">\r\n                                    Detail Product\r\n                                </li>\r\n                            </ol>\r\n                        </nav>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"row\">\r\n                    <div class=\"col-md-6\">\r\n                        <div class=\"card\">\r\n                            <img :src=\" '../assets/img/' + product.gambar \" class=\"img-fluid shadow\"/>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"col-md-6\">\r\n                        <h2>{{ product.nama }}</h2>\r\n                        <h5>Harga: Rp. {{ product.harga }}</h5>\r\n                        <br/>\r\n                        <form v-on:submit.prevent>\r\n                            <div class=\"form-group\">\r\n                                <label for=\"jumlah_pesanan\">Jumlah Pesan</label>\r\n                                <input type=\"number\" class=\"form-control\" v-model=\"pesan.jumlah_pesanan\">\r\n                            </div>\r\n                            <div class=\"form-group\">\r\n                                <label for=\"keterangan\">Keterangan</label>\r\n                                <textarea \r\n                                v-model=\"pesan.keterangan\"\r\n                                class=\"form-control\"\r\n                                placeholder=\"isi keterangan produk yang dipesan..\">\r\n                                </textarea>\r\n                            </div>\r\n                            <button type=\"submit\" class=\"btn btn-success\" @click=\"pesanan\"><b-icon-cart></b-icon-cart> Pesan</button>\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n                \r\n\r\n            </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarMenu from \"@/components/NavbarMenu.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    name: \"FoodDetail\",\r\n    components: {\r\n        NavbarMenu,\r\n    },\r\n    data() {\r\n        return {\r\n            product:{},\r\n            pesan: {}\r\n        };\r\n    },\r\n    methods: {\r\n        setProduct(data) {\r\n            this.product = data;\r\n        },\r\n        pesanan() {\r\n            console.log(this.pesan);\r\n        if(this.pesan.jumlah_pesanan) {\r\n            this.pesan.products = this.product;\r\n            axios\r\n            .post(\"http://localhost:3000/keranjangs/\", this.pesan)\r\n            .then(() => {\r\n                this.$route.push({path: \"/keranjang\"})\r\n                this.$toast.success('Pesanan Berhasil di pesan', {\r\n                    type: 'success', \r\n                    position: 'top-right', \r\n                    duration: 3000, \r\n                    dismissable: true \r\n                });\r\n                })\r\n            .catch((error) => console.log(error))\r\n        }else {\r\n            this.$toast.error('Pesanan Gagal pesanan harus di isi', {\r\n                    type: 'error', \r\n                    position: 'top-right', \r\n                    duration: 3000, \r\n                    dismissable: true \r\n                });\r\n        }\r\n        },\r\n    },\r\n    mounted() {\r\n    axios\r\n    .get(\"http://localhost:3000/products/\"+this.$route.params.id)\r\n    .then((response) => this.setProduct(response.data))\r\n    .catch((error) => console.log(error))\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n .nav-breadcrumb {\r\n    padding: 40px 0;\r\n }\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}